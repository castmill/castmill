{
  "name": "@castmill/electron-player",
  "version": "1.0.0",
  "description": "Castmill Player Electron App",
  "main": "./out/main/index.js",
  "author": "Castmill AB",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "yarn typecheck && electron-vite build",
    "build2": "electron-vite build -l info",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@castmill/cache": "*",
    "@castmill/device": "*",
    "@castmill/player": "*",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "check-disk-space": "^3.4.0",
    "electron": "^32.2.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.7",
    "macaddress": "^0.5.3",
    "systeminformation": "^5.22.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.9",
    "electron": "28.2.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^9.19.0",
    "eslint-plugin-solid": "^0.13.1",
    "prettier": "^3.2.5",
    "solid-js": "^1.8.22",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vite-plugin-solid": "^2.9.1",
    "vitest": "^1.5.3"
  }
}
